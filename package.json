{
  "name": "twitch-chatlog",
  "version": "6.1.0",
  "description": "Fetch the chat log to a Twitch VOD.",
  "main": "lib/index.js",
  "bin": {
    "twitch-chatlog": "./bin/twitch-chatlog"
  },
  "engines": {
    "node": ">=7.10.1"
  },
  "scripts": {
    "test": "npm run lint && nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint lib/* bin/* test/* --ext js,json",
    "start": "node bin/twitch-chatlog"
  },
  "keywords": [
    "twitch",
    "vod",
    "log",
    "chat",
    "replay"
  ],
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "iterm2-version": "^4.2.0",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.2",
    "string-replace-async": "^1.2.1",
    "term-img": "^4.1.0",
    "yargs": "^14.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freaktechnik/twitch-chatlog"
  },
  "homepage": "https://github.com/freaktechnik/twitch-chatlog",
  "bugs": {
    "url": "https://github.com/freaktechnik/twitch-chatlog/issues"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@freaktechnik/eslint-config-node": "^7.0.0",
    "@freaktechnik/eslint-config-test": "^7.0.0",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "nyc": "^14.1.1"
  },
  "sideEffects": false,
  "ava": {
    "helpers": [
      "test/api/*"
    ]
  }
}
