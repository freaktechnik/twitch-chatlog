  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.2.0",
    "hex-to-color-name": "^1.0.1",
    "node-fetch": "^2.0.0-alpha.8",
    "ora": "^1.3.0",
    "yargs": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freaktechnik/twitch-chatlog"
  },
  "homepage": "https://github.com/freaktechnik/twitch-chatlog",
  "bugs": {
    "url": "https://github.com/freaktechnik/twitch-chatlog/issues"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@freaktechnik/eslint-config-node": "^5.2.1",
    "@freaktechnik/eslint-config-test": "^5.2.1",
    "ava": "^0.23.0",
    "codecov": "^3.0.0",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "nyc": "^11.3.0"
  }
